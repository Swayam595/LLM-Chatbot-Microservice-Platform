                        "title": "Password"
                    },
                    "role": {
                        "type": "string",
                        "enum": [
                            "admin",
                            "user"
                        ],
                        "title": "Role",
                        "default": "user"
                    }
                },
                "type": "object",
                "required": [
                    "username",
                    "email",
                    "password"
                ],
                "title": "UserCreate",
                "description": "Schema for creating a user"
